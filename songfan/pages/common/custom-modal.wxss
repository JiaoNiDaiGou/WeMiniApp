/* pages/common/feedback-modal.wxss */
@import '../../lib/icon.wxss';
@import '../../lib/weui.wxss';
@import '../../app.wxss';

.fui-custom-modal-mask {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 9999;
}

.fui-custom-modal-title {
  justify-content:space-around;
  text-align: center;
  border-bottom: 1px  groove gray
}

.fui-custom-modal-button {
  margin-top: 10px;
  margin-bottom: 10px;
  margin-left: 15px;
  margin-right: 15px;
}

.fui-custom-modal-content {
  display: flex;
  flex-direction: column;
  width: 90%;
  /* height: 80%; */
  background-color: #fff;
  border-radius: 10rpx;
}